{% if profiles is not empty %}
{% for profile in profiles %}
<img src="{{ profile.getPicture }}" height="100" width="100">
<ul>
  <li><a href="/viewprofile/{{ profile.getFirstName }}/{{ profile.getId }}/{{ user_id }}">{{ profile.getFirstName }} {{ profile.getLastName }}</a></li>
</ul>
{% endfor %}
{% endif %}

{% if msg is not empty %}
{{ msg }}
{% endif %}
